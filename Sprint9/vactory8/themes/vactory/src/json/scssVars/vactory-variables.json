{
	"facebook": "#365397",
	"twitter": "#00a9f1",
	"linkedin": "#006db3",
	"google_plus": "#e0452c",
	"youtube": "#ff3333",
	"vimeo": "#48b6ed",
	"pinterest": "#ce1a19",
	"skype": "#00acf4",
	"instagram": "#396d9a",
	"github": "#2f2f2f",
	"drupal": "#0c76ab",
	"bitbucket": "#205081",
	"media-colors": {
		"map-merge(\n  \"facebook\"": "$facebook",
		"\"twitter\"": "$twitter",
		"\"linkedin\"": "$linkedin",
		"\"google_plus\"": "$google_plus",
		"\"youtube\"": "$youtube",
		"\"vimeo\"": "$vimeo",
		"\"pinterest\"": "$pinterest",
		"\"skype\"": "$skype",
		"\"instagram\"": "$instagram",
		"\"github\"": "$github",
		"\"drupal\"": "$drupal",
		"\"bitbucket\"": "$bitbucket\n)"
	},
	"icomoon-font-path": "\"../fonts/icons-font/vactory-icons-font/fonts\" !default",
	"hamburger-padding-x": "5px",
	"hamburger-padding-y": "4px",
	"hamburger-layer-width": "24px",
	"hamburger-layer-height": "1px",
	"hamburger-layer-spacing": "4px",
	"hamburger-layer-color": "$black",
	"hamburger-layer-border-radius": "5px",
	"hamburger-hover-opacity": ".7",
	"hamburger-active-layer-color": "$hamburger-layer-color",
	"hamburger-active-hover-opacity": "$hamburger-hover-opacity",
	"hamburger-hover-use-filter": "false",
	"hamburger-hover-filter": "opacity(50%)",
	"hamburger-active-hover-filter": "$hamburger-hover-filter",
	"transition-duration": "250ms",
	"ease-in-out-circ": "cubic-bezier(.27, .01, .38, 1.06)",
	"bg-filter-color": "$black",
	"paragraph-lg": "100px",
	"paragraph-sm": "64px",
	"splash-lg": "50px",
	"splash-sm": "20px",
	"font-family-opensans": "'Open Sans' !default"
}